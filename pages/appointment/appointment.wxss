page { -webkit-user-select:none; user-select:none; width:100%; overflow:hidden; height:100%; }
.container {width:100%; height:100%;position:relative; }
.box{width:100%;position:absolute;top:10px;left:0px;z-index:111}
.shadowBox{width:100%;position:absolute;top:10px;left:0px;z-index:11;background:rgba(255,255,255,.1);height:90px;}
.inputBox{height:44px;margin-left:16px;clear:both;overflow:hidden}
text{height:44px;font-family: PingFangSC-Regular;font-size: 17px;color: #FFFFFF;letter-spacing: 0;width:35px;float:left;line-height:44px;}
input{height:44px;line-height:44px;margin-left:55px;font-size: 17px;float:left;color:#fff}
.info{opacity: 0.6;font-family: PingFangSC-Regular;font-size: 14px;color: #FFFFFF;text-align: center;width:270px;margin:auto;margin-top:11px;}
