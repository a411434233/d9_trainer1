.contaierBox{background:#f6f6f6;position:fixed;width:100%;height:100%;}
.title{font-size: 30px; color: #333333;margin-top:20px;margin-left:27px;margin-bottom:30px;}
.confirm{background:#E74747;border-radius:100px;height:45px;line-height:45px;width:90%;margin:auto;text-align:center;color:#fff;font-size:16px;box-shadow:0px 0px 100px 2px rgba(231,71,71,.2);margin-top:70px;}
.userAgree{padding:0px 27px;font-size: 12px; color: #444444;margin-top:-5px}
.userAgree text{color:#3C8DFF}
.inputBox{padding:0px 27px;height:30px;line-height:30px;clear:both;margin-bottom:30px;}
.inputBox image{width:20px;float:left;margin-right:10px;margin-top:5px;}
.inputBox .name,input{float:left;margin-right:20px;font-size:14px;color:#444444;}
.inputBox .name{width:50px;}
input{width:150px; height:30px;margin-right:0;line-height:30px;border-bottom:1px solid #F0F0F0;}
input::placeholder{font-size: 14px; color: #CCCCCC;}
.getCode{font-size:12px;color:#FFFFFF;background:#FF5656;width:55px;height:30px;line-height:30px;border-radius:100px;float:right;text-align:center;}
.getCode.disable{background:#999}
.unChose,.chose{position:relative;width:10px; height:10px; float:left; border:2px solid #FF5656; border-radius:50%; margin-right:7px;margin-top:2px;}
.chose:before{content:"\2713";top:50%;width:15px;height:15px;color:#FF5656;position:absolute;left:50%;margin-left:-5px;margin-top:-8px;}