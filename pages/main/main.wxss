.main{padding-left:18px;padding-top:30px;}
.main .title{font-size: 20px;color: #555555;height:30px;line-height:30px;margin-bottom:10px;}
page{position:fixed;width:100%;height:100%;top:0px;left:0pz;}
.cardBox{background:#1e2237;border-radius:10px;padding:15px;width:calc(100% - 98px);box-shadow:0px 3px 15px 0px rgba(0,0,0,0.5);margin-bottom:30px;overflow:hidden}
.personalTrainingBox{width:calc(100% - 68px);position:relative;margin-bottom:30px;box-shadow:0px 3px 15px 0px rgba(0,0,0,0.5);border-radius:10px;overflow:hidden}
.lesson{position:absolute;bottom:10px;right:10px;width:47px;border-radius:10px;overflow:hidden;}
.activity{box-shadow:0px 3px 15px 0px rgba(0,0,0,0.5);margin-bottom:30px;border-radius:10px;overflow:hidden;width:calc(100% - 68px);}
.venues{width:calc(100% - 68px);box-shadow:0px 3px 15px 0px rgba(0,0,0,0.5);margin-bottom:30px;border-radius:7px;overflow:hidden;}
.mybox{width:calc(100% - 68px);margin-bottom:30px}
.userInfo{margin-top:10px;}
.userBg{width:100%;box-shadow:0px 3px 15px 0px rgba(0,0,0,0.5);border-radius:7px;overflow:hidden;position:relative;}
.avatar{width:50px;height:50px;position:absolute;left:12px;top:16px;border-radius:50%;}
.navLists{width:100%;margin-top:30px;}
.name{font-size: 18px; color: #FFFFFF; text-shadow: 0 1px 1px rgba(0,0,0,0.50);position:absolute;left:72px;top:29px}
.righeNav{position:fixed;right:0px;top:40px;height:100%;overflow:auto;}
.righeNav .titBox{font-size: 12px; color: #666666; text-align:right;clear:both}
.scale{height:2px;margin-bottom:6px;clear:both;float:right;background: #CCCCCC;}
.titBox .tit{float:right;height:20px;line-height:20px;margin-right:3px;}
.titBox .lineBox{float:right;width:0px;overflow:hidden}
.titBox.boxCard_card .lineBox,.titBox.personal_personalTraining .lineBox,.titBox.lesson_lesson .lineBox,.titBox.activity_activity .lineBox,.titBox.venues_venues .lineBox,.titBox.my_my .lineBox{width:auto;}
.titBox.boxCard_card .tit,.titBox.personal_personalTraining .tit,.titBox.lesson_lesson .tit,.titBox.activity_activity .tit,.titBox.venues_venues .tit,.titBox.my_my .tit{color:#ED3839;float:left}
.titBox.boxCard_card .scale,.titBox.personal_personalTraining .scale,.titBox.lesson_lesson .scale,.titBox.activity_activity .scale,.titBox.venues_venues .scale,.titBox.my_my .scale{background: #ED3839;}
.activity{position:relative}
.activity .time{font-size: 10px; color: #fff; line-height:14px;position:absolute;bottom:10px;left:22px;opacity:0.5}
::-webkit-scrollbar {width: 0;height: 0;color: transparent;}
.cardBox image{width:100%}
.cardBox .img{position:relative;padding-bottom:15px;margin-bottom:3px}
.cardBox .img .bottom{width:40px;height:20px;bottom:2px;position:absolute;left:50%;margin-left:-20px;border-radius:0 0 20px 20px;}
.cardBox .year .bottom{background:#3d48a0;}
.cardBox .month .bottom{background:#ce9ab2;}
.cardBox .season .bottom{background:#63b7a9;}
.cardBox .all .bottom{background:#c9b176;}
.sales{text-align:right;font-size: 12px; color: #FFFFFF;display:block;width:100%;text-align:right;margin-top:4px;}
.limit{text-align:right;font-size: 10px; color: #FFFFFF;opacity: 0.5;display:block;margin-bottom:15px;margin-top:2px;}